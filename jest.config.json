{
    "verbose": true,
    "modulePathIgnorePatterns": ["dist"],
    "coverageReporters": ["lcov", "text"],
    "coveragePathIgnorePatterns": ["babel.config.js", "test", "node_modules", "coverage"],
    "collectCoverage": true,
    "coverageThreshold": {
        "global": {
            "branches": 70,
            "functions": 70,
            "lines": 70,
            "statements": 70
        }
    },
    "setupFiles": ["<rootDir>/test/setEnvVars.js"]
}
